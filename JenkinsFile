pipeline {
    agent any
    stages {
        stage("Checkout") {
            steps{
                git 'https://github.com/m-browntown/example-java-maven.git'
            }
        }

        stage("SrcClr Scan") {
            steps{
                powershell label: '', Set-ExecutionPolicy AllSigned -Scope Process -Force;
                powershell label: '', script: 'iex ((New-Object System.Net.WebClient).DownloadString(\'https://download.srcclr.com/ci.ps1\')); srcclr scan'
            }
        }   
    }
}