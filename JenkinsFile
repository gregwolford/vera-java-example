pipeline {
    agent any
    stages {
        stage("Checkout") {
            steps{
                git 'https://github.com/m-browntown/example-java-maven.git'
            }
        }

        stage("SrcClr Scan") {
            steps{
                script {
                    if(isUnix()){
                        echo 'Is Unix'
                    } else{
                        echo 'Is Windows'
                    }
                }
                
            }
        }   
    }
}